{
  "python.defaultInterpreterPath": ".venv/bin/python",
  "python.analysis.typeCheckingMode": "basic",
  "flake8.args": [
    "--max-line-length=100",
    "--exclude=ali_old.py",
    "--ignore=E203,E501,E722,F541,F401,W291,W293,W503"
  ],
  "ansible.python.interpreterPath": ".venv/bin/python",
  "ansible.validation.enabled": true,
  "ansible.validation.lint.enabled": true,
  "ansible.validation.lint.path": ".venv/bin/ansible-lint",
  "ansible.validation.lint.arguments": "-c dev/.ansible-lint.yml",
  "ansible.playbook.path": "cloudy/playbooks",
  "ansible.ansiblePath": ".venv/bin/ansible",
  "ansible.ansibleLint.path": ".venv/bin/ansible-lint",
  "ansible.workspaceFolders": [
    "cloudy"
  ],
  "files.associations": {
    "*.yml": "ansible",
    "*.yaml": "ansible"
  },
  "cSpell.import": [
    "dev/.cspell.json"
  ],
  "terminal.integrated.env.osx": {
    "PATH": "${workspaceFolder}/.venv/bin:${env:PATH}"
  },
  "terminal.integrated.env.linux": {
    "PATH": "${workspaceFolder}/.venv/bin:${env:PATH}"
  }
}
